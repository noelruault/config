export DEFAULT_PROJECT_PATH="$HOME/Projects/masmovil-dockerize"
alias dk="$HOME/Projects/masmovil-dockerize/docker/run.sh $@"
alias dkclean="sh $HOME/.zshrc-config/docker/docker_clean_all.sh"
alias dkcreate="sh $HOME/.zshrc-config/docker/docker_turn_on.sh"
alias dklightclean="sh $HOME/.zshrc-config/docker/docker_light_clean.sh"
alias dkstatus="docker ps -q | xargs  docker stats --no-stream"
